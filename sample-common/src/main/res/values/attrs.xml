<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ShapeView">

        <attr name="shape" format="enum">
            <enum name="rectangle" value="0" />
            <enum name="ring" value="1" />
            <enum name="triangle" value="2" />
        </attr>

        <attr name="border_color" format="color" />
        <attr name="border_width" format="dimension" />

    </declare-styleable>


    <declare-styleable name="CustomSwipeRefreshLayout">

        <attr name="progressBackground" format="color" />
        <attr name="progressColorScheme" format="color" />
        <attr name="progressColorSchemes" format="reference" />

    </declare-styleable>

    <declare-styleable name="StatusBar">

        <attr name="mode" format="enum">
            <enum name="light" value="0" />
            <enum name="dark" value="1" />
        </attr>

    </declare-styleable>

</resources>
